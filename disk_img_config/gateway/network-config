# Some additional examples are commented out below

version: 2
ethernets:
  eth0:
    dhcp4: false
    addresses: [10.0.0.1/24]
wifis:
  wlan0:
    dhcp4: true
    optional: true
    access-points:
      "<SSID_NAME>":
        password: "<SSID_PASSWD>"
